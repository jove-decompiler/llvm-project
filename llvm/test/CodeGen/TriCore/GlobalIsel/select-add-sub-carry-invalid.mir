# RUN: llc -mtriple=tricore -global-isel -global-isel-abort=2 \
# RUN:     -pass-remarks-missed='gisel*' -run-pass=instruction-select \
# RUN:     -verify-machineinstrs %s -o - 2>&1 \
# RUN: | FileCheck %s --check-prefix=ERROR --implicit-check-not remark:

# ERROR: remark: <unknown>:0:0: cannot select: %2:dataregbank(s32), %3:statusregbank(s1) = G_UADDO %0:dataregs, %1:dataregs (in function: add_carry_out_clobbered)
# ERROR: remark: <unknown>:0:0: cannot select: %2:dataregbank(s32), %3:statusregbank(s1) = G_UADDO %0:dataregs, %1:dataregs (in function: add_carry_out_clobbered_implicit)
# ERROR: remark: <unknown>:0:0: cannot select: %2:dataregbank(s32), %3:statusregbank(s1) = G_USUBO %0:dataregs, %1:dataregs (in function: sub_carry_out_clobbered)
# ERROR: remark: <unknown>:0:0: cannot select: %2:dataregbank(s32), %3:statusregbank(s1) = G_USUBO %0:dataregs, %1:dataregs (in function: sub_carry_out_clobbered_implicit)

---
name:            add_carry_out_clobbered
legalized:       true
regBankSelected: true
registers:
  - { id: 0, class: dataregbank }
  - { id: 1, class: dataregbank }
  - { id: 2, class: dataregbank }
  - { id: 3, class: statusregbank }
  - { id: 4, class: dataregbank }
  - { id: 5, class: statusregbank }
  - { id: 6, class: dataregbank }
  - { id: 7, class: dataregbank }
body:             |
  bb.0:
    liveins: $d4, $d5
    %0(s32) = COPY $d4
    %1(s32) = COPY $d5
    %2(s32), %3(s1) = G_UADDO %0, %1
    %4(s32), %5(s1) = G_UADDE %0, %1, %3
    %6(s1) = COPY %3(s1)
    %7(s32) = G_ZEXT %6(s1)
    $d4 = COPY %7(s32)
    $d5 = COPY %4(s32)

...

---
name:            add_carry_out_clobbered_implicit
legalized:       true
regBankSelected: true
registers:
  - { id: 0, class: dataregbank }
  - { id: 1, class: dataregbank }
  - { id: 2, class: dataregbank }
  - { id: 3, class: statusregbank }
  - { id: 4, class: dataregbank }
  - { id: 5, class: dataregbank }
  - { id: 6, class: dataregbank }
body:             |
  bb.0:
    liveins: $d4, $d5
    %0(s32) = COPY $d4
    %1(s32) = COPY $d5
    %2(s32), %3(s1) = G_UADDO %0, %1
    %4(s32) = G_ASHR %0, %1
    %5(s1) = COPY %3(s1)
    %6(s32) = G_ZEXT %5(s1)
    $d4 = COPY %6(s32)
    $d5 = COPY %4(s32)

...

