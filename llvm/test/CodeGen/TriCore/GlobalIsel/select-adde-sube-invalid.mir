---
name:            adde_const_non_zero
legalized:       true
regBankSelected: true
registers:
  - { id: 0, class: dataregbank }
  - { id: 1, class: dataregbank }
  - { id: 2, class: dataregbank }
  - { id: 3, class: dataregbank }
  - { id: 4, class: dataregbank }
  - { id: 5, class: dataregbank }
body:             |
  bb.0:
    liveins: $d4, $d5
    %0(s32) = COPY $d4
    %1(s32) = COPY $d5
    %2(s32) = G_CONSTANT i32 1
    %3(s1) = G_TRUNC %2(s32)
    %4(s32), %5(s1) = G_UADDE %0, %1, %3
    $d4 = COPY %4(s32)
...

